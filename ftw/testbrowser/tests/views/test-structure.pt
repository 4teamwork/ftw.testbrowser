<html xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      metal:use-macro="context/main_template/macros/master"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="ftw.testbrowser">
    <body>

        <div metal:fill-slot="main" >

            <div class="foo">
                <span class="bar">Bar in Foo</span>
                <a href="#">Link in <em>Foo</em></a>
            </div>

            <span class="bar">Bar</span>

            <div class="baz">
                <a href="#">Link in Baz</a>
            </div>

            <div id="some-links">
                <a href="#">First link</a>
                <a href="#">Second link</a>
                <a href="#">Third link</a>
            </div>

            <ul id="list-of-links">
                <li><a href="#">Link of first item</a></li>
                <li><a href="#">Link of second item</a></li>
            </ul>

            <dl id="definition-list-of-links">
                <dt>Mails</dt>
                <dd><a href="http://mail.google.com">mail.google.com</a></dd>
                <dt>Search</dt>
                <dd><a href="http://www.google.com">google.com</a></dd>
                <dt>Maps</dt>
                <dd><a href="http://maps.google.com">maps.google.com</a></dd>
            </dl>

            <div class="non-breaking">Non&nbsp;breaking&nbsp;spaces.</div>

            <div class="empty"></div>

            <div id="nested">
                <em class="outer">outer</em>
                <div><em class="inner">inner</em></div>
            </div>

        </div>

    </body>
</html>
